@model IEnumerable<MileStone2A.Models.ProductCategory>

@{
    ViewBag.Title = "Accessories";
}

<h2>Accessories</h2>

<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(@item.Name, "Details", new { id = item.ProductCategoryID }, new { @class = "list-brand" })
                <br />
            </td>
        </tr>
    }

</table>
